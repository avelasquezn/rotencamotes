%head
  = formtastic_stylesheet_link_tag
%h1 Editing award_category
- form_for(@award_category) do |f|
  = f.error_messages
  - semantic_form_for(@award_category) do |f|
    = f.error_messages
    - f.inputs do
      = f.input :name
    - f.buttons do
      = f.commit_button 'Update'
  = link_to 'Show', @award_category
  |
  \#{link_to 'Back', award_categories_path}
