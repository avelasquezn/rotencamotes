%h1 Editing video
- form_for(@video) do |f|
  = f.error_messages
  %p
    = f.label :title
    %br/
    = f.text_field :title
  %p
    = f.label :description
    %br/
    = f.text_area :description
  %p
    = f.label :video_type
    %br/
    = f.text_field :video_type
  %p
    = f.label :content
    %br/
    = f.text_area :content
  %p
    = f.submit 'Update'
= link_to 'Show', @video
|
\#{link_to 'Back', videos_path}
