<head>
  <%= formtastic_stylesheet_link_tag %>
</head>

<h1>New genre</h1>

<% semantic_form_for(@genre) do |f| %>
  <%= f.error_messages %>
  <% f.inputs do %>
    <%= f.input :name %>
  <% end %>
  <% f.buttons do %>
    <%= f.commit_button 'Create' %>
  <% end %>
<% end %>

<%= link_to 'Back', genres_path %>

