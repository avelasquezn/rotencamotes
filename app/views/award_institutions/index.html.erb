<head>
  <%= formtastic_stylesheet_link_tag %>
</head>

<h1>Listing award_institutions</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th>Bio</th>
    <th>Url</th>
    <th>Logo file name</th>
    <th>Logo content type</th>
    <th>Logo file size</th>
    <th>Logo updated at</th>
  </tr>

<% @award_institutions.each do |award_institution| %>
  <tr>
    <td><%=h award_institution.name %></td>
    <td><%=h award_institution.description %></td>
    <td><%=h award_institution.bio %></td>
    <td><%=h award_institution.url %></td>
    <td><%=h award_institution.logo_file_name %></td>
    <td><%=h award_institution.logo_content_type %></td>
    <td><%=h award_institution.logo_file_size %></td>
    <td><%=h award_institution.logo_updated_at %></td>
    <td><%= link_to 'Show', award_institution %></td>
    <td><%= link_to 'Edit', edit_award_institution_path(award_institution) %></td>
    <td><%= link_to 'Destroy', award_institution, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New award_institution', new_award_institution_path %>

